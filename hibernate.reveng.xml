<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_admin" catalog="classroom"
		class="com.classroom.model.Admin">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_classroom" catalog="classroom"
		class="com.classroom.model.Classroom">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_reservation" catalog="classroom"
		class="com.classroom.model.Reservation">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="t_user" catalog="classroom"
	class="com.classroom.model.User">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>